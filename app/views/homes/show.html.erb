<div class="page-header">
  <%= link_to homes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_home_path(@home), class: 'btn btn-info' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>| Tiny Home details |</h1>
</div>

<dl class="dl-horizontal">
  <dt>Address:</dt>
  <dd><%= @home.address %></dd>

  <dt>City:</dt>
  <dd><%= @home.city %></dd>

  <dt>State:</dt>
  <dd><%= @home.state %></dd>

  <dt>Zip:</dt>
  <dd><%= @home.zip %></dd>

  <dt>Bedrooms:</dt>
  <dd><%= @home.bedrooms %></dd>

  <dt>Baths:</dt>
  <dd><%= @home.baths %></dd>

  <dt>Square feet:</dt>
  <dd><%= @home.square_feet %></dd>

  <dt>Price:</dt>
  <dd><%= @home.price %></dd>

  <dt>Description:</dt>
  <dd><%= @home.description %></dd>

  <dt>Image:</dt>
  <dd><%= image_tag(@home.image_url, size: "420") %></dd>

</dl>
